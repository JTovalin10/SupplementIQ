{
  "name": "supplementiq",
  "version": "0.1.0",
  "private": true,
  "description": "Transparency engine for supplement industry - crowdsourced data with bioavailability calculations",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "fix-all": "npm run lint:fix && npm run format",
    "sync-env": "node sync-env.js",
    "clean": "rm -rf node_modules .next",
    "prepare": "npx husky install",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "express-validator": "^7.2.0",
    "fast-levenshtein": "^3.0.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "levenshtein": "^1.0.5",
    "lucide-react": "^0.460.0",
    "next": "15.5.4",
    "node-addon-api": "^8.5.0",
    "node-cron": "^4.2.1",
    "ipaddr.js": "^2.1.0",
    "validator": "^13.12.0",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^5.8.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^2.1.9",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
