{
  "name": "supplementiq",
  "version": "0.1.0",
  "private": true,
  "description": "Transparency engine for supplement industry - crowdsourced data with bioavailability calculations",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "fix-all": "npm run lint:fix && npm run format",
    "sync-env": "node sync-env.js",
    "clean": "rm -rf node_modules .next",
    "prepare": "npx husky install",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/react": "^5.0.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "fast-levenshtein": "^3.0.0",
    "ioredis": "^5.8.2",
    "ipaddr.js": "^2.1.0",
    "js-cookie": "^3.0.5",
    "levenshtein": "^1.0.5",
    "lucide-react": "^0.460.0",
    "nanoid": "^5.1.6",
    "next": "^16.0.0-canary.18",
    "node-addon-api": "^8.5.0",
    "node-cron": "^4.2.1",
    "picocolors": "^1.1.1",
    "react": "^18",
    "react-dom": "^18",
    "redis": "^5.8.3",
    "redis-errors": "^1.2.0",
    "styled-components": "^6.1.19",
    "webidl-conversions": "^8.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
